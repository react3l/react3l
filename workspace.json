{
  "version": 2,
  "projects": {
    "@react3l-common": {
      "root": "libs/@react3l/common",
      "sourceRoot": "libs/@react3l/common/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/@react3l/common/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/@react3l/common"
          ],
          "options": {
            "jestConfig": "libs/@react3l/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/@react3l/common",
            "tsConfig": "libs/@react3l/common/tsconfig.lib.json",
            "packageJson": "libs/@react3l/common/package.json",
            "main": "libs/@react3l/common/src/index.ts",
            "assets": [
              "libs/@react3l/common/*.md"
            ]
          }
        }
      }
    },
