{
  "name": "@react3l/react3l",
  "version": "5.0.0-alpha.1",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "scripts": {
    "start-storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook -o build/",
    "test": "jest",
    "watch": "NODE_ENV=development rollup -c -w",
    "rm-old-build": "rm -rf dist/",
    "cp-files": "cp package.json tsconfig.json README.md dist/",
    "prebuild": "run-s rm-old-build",
    "build": "NODE_ENV=production rollup -c",
    "tsc": "tsc",
    "postbuild": "run-s cp-files tsc",
    "extract": "i18next-extractor -i src/ -o src/i18n/ -p src/i18n/ extract",
    "merge": "i18next-extractor -i src/ -o src/i18n/ -p src/i18n/ merge"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint --fix src/**/*.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@react3l/axios-observable": "*",
    "axios": "*",
    "i18next": "*",
    "lodash": "*",
    "moment": "*",
    "path": "*",
    "react": "*",
    "react-i18next": "*",
    "reactn": "*",
    "reflect-metadata": "*",
    "rxjs": "*",
    "yup": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@react3l/advanced-filters": "^1.1.0",
    "@react3l/axios-observable": "^1.1.4",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^6.0.21",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/ts-nameof": "^4.2.1",
    "@types/yup": "^0.29.6",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "antd": "^4.5.4",
    "axios": "^0.19.2",
    "babel-jest": "^26.6.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "bootstrap": "^4.5.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "husky": "^4.3.0",
    "i18next": "^19.6.3",
    "jest": "^26.6.2",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "node-directory-indexing": "^0.0.4",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-test-renderer": "^17.0.1",
    "reactn": "^2.2.7",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.33.1",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rxjs": "^6.6.2",
    "style-loader": "^1.2.1",
    "ts-nameof.macro": "^4.2.2",
    "typescript": "^3.9.5",
    "yup": "^0.29.3"
  }
}
