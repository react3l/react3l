{
  "name": "react3l",
  "version": "4.0.0-test-rc2",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start-storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook -o build/",
    "test": "react-scripts test",
    "watch": "NODE_ENV=development tsc -w",
    "rm-old-build": "rm -rf dist/",
    "enable-emit": "./react3l.js enable-emit",
    "disable-emit": "./react3l.js disable-emit",
    "cp-package-json": "cp package.json dist/",
    "prebuild": "run-s rm-old-build enable-emit",
    "build": "NODE_ENV=production tsc",
    "postbuild": "run-s cp-package-json disable-emit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "antd": "^4.3.4",
    "axios": "^0.19.2",
    "axios-observable": "^1.1.3",
    "i18next": "^19.5.0",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "reactn": "^2.2.7",
    "redux": "^4.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "ts-nameof.macro": "^4.2.2"
  },
  "devDependencies": {
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/reflect-metadata": "^0.1.0",
    "commander": "^5.1.0",
    "css-loader": "^3.6.0",
    "firebase-tools": "^8.4.3",
    "i18next-extractor": "^0.3.4",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "react3l-cli": "^0.4.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
